<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!-- Whether SELinux status should be shown or not. -->
    <bool name="config_show_selinux_status">true</bool>

    <!-- Device specific doze package -->
    <string name="config_customDozePackage">org.lineageos.settings/org.lineageos.settings.doze.DozeSettingsActivity</string>

    <!-- Whether to show min/max refresh rate in display settings -->
    <bool name="config_show_refresh_rate_controls">true</bool>
    <bool name="config_supports_dynamic_refresh_rate_controls">true</bool>

  <!-- Battery Health -->
    <string name="config_batteryRemainingCapacityNode">/sys/class/power_supply/battery/charge_full</string>
    <string name="config_batteryDesignCapacityNode">/sys/class/power_supply/battery/charge_full_design</string>
    <string name="config_batteryChargeCycles">/sys/class/power_supply/battery/cycle_count</string>
</resources>
